<?xml version="1.1" ?>
<settings>
    <global>
        <ezcastbar>
            <enable>true</enable>
            <image_bar>gui/ezcastbar/castbar_bar.png</image_bar>
            <image_frame>gui/ezcastbar/castbar_frame.png</image_frame>
            <offset>
                <x>0</x>
                <y>-200</y>
            </offset>
            <size>
                <height>25</height>
                <width>300</width>
            </size>
        </ezcastbar>
        <ezmount>
            <enable>true</enable>
            <name>Raptor</name>
        </ezmount>
        <ezparty>
            <buffs>
                <enable>true</enable>
                <global_scale>1</global_scale>
                <member>
                    <horizontal_limit>10</horizontal_limit>
                    <location>below</location>
                    <offset>
                        <x>0</x>
                        <y>0</y>
                    </offset>
                    <scale>1</scale>
                    <vertical_limit>2</vertical_limit>
                </member>
                <player>
                    <horizontal_limit>10</horizontal_limit>
                    <location>below</location>
                    <offset>
                        <x>0</x>
                        <y>0</y>
                    </offset>
                    <scale>1</scale>
                    <vertical_limit>2</vertical_limit>
                </player>
            </buffs>
            <enable>true</enable>
            <layout>
                <draggable>true</draggable>
                <vertical_spacing>50</vertical_spacing>
            </layout>
            <member_frame>
                <enable>true</enable>
                <image>gui/ezparty/member_frame.png</image>
                <linked>true</linked>
                <offset>
                    <x>0</x>
                    <y>0</y>
                </offset>
                <size>
                    <height>339</height>
                    <width>1649</width>
                </size>
            </member_frame>
            <player_frame>
                <duplicate>
                    <enable>false</enable>
                </duplicate>
                <enable>true</enable>
                <image>gui/ezparty/player_frame.png</image>
                <linked>true</linked>
                <offset>
                    <x>0</x>
                    <y>0</y>
                </offset>
                <size>
                    <height>339</height>
                    <width>1649</width>
                </size>
                <unique>true</unique>
            </player_frame>
            <scale>1</scale>
            <target_frame>
                <above_member_frames>
                    <enable>false</enable>
                    <offset>
                        <x>0</x>
                        <y>-10</y>
                    </offset>
                </above_member_frames>
                <enable>true</enable>
                <image>gui/ezparty/target_frame.png</image>
                <size>
                    <height>339</height>
                    <width>1649</width>
                </size>
                <top_center_screen>
                    <enable>true</enable>
                    <offset>
                        <x>0</x>
                        <y>0</y>
                    </offset>
                </top_center_screen>
            </target_frame>
        </ezparty>
        <eztargeting>
            <enable>true</enable>
        </eztargeting>
    </global>
</settings>
